{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "Also Very Cool Build",
            "command": "/usr/bin/g++",
            "args": [
                "-o",
                "${workspaceFolder}/SingleNet",
                "${workspaceFolder}/main.cpp",
                "${workspaceFolder}/Tools/Datasets.cpp",
                "${workspaceFolder}/Tools/ProgressBar.cpp",
                "-std=gnu++17",
                "-g"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ],
    "version": "2.0.0"
}